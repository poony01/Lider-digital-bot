{
  "version": 4,
  "builds": [
    {
      "src": "webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/pix.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/pix",
      "dest": "/api/pix.js"
    },
    {
      "src": "/(.*)",
      "dest": "/webhook.js"
    }
  ]
}
